{
  "name": "node-scrapy",
  "description": "Simple, lightweight and expressive web scraping with Node.js",
  "eslintConfig": {
    "extends": [
      "airbnb",
      "standard"
    ],
    "root": true,
    "rules": {
      "max-len": [
        "error",
        80,
        2,
        {
          "ignoreComments": false,
          "ignoreRegExpLiterals": true,
          "ignoreStrings": true,
          "ignoreTemplateLiterals": true,
          "ignoreUrls": true
        }
      ]
    }
  },
  "keywords": [
    "web",
    "html",
    "css",
    "selector",
    "scraping",
    "scrape",
    "scraper",
    "scrapy",
    "crawler"
  ],
  "version": "0.4.0-alpha.1",
  "author": "Stefan Maric <me@stefanmaric.com>",
  "repository": "eeshi/node-scrapy",
  "bugs": {
    "url": "http://github.com/eeshi/node-scrapy/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/eeshi/node-scrapy",
  "readmeFilename": "README.md",
  "engines": {
    "node": ">=6.9.0"
  },
  "main": "lib/index.js",
  "dependencies": {
    "css-select": "^1.2.0",
    "dom-serializer": "^0.1.0",
    "htmlparser2": "^3.9.2"
  },
  "devDependencies": {
    "echint": "^4.0.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "tap": "^10.0.0"
  },
  "scripts": {
    "test": "echint && eslint . && tap 'test/**/*.test.js'"
  }
}
